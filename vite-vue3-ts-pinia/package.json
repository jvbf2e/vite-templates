{
  "private": true,
  "name": "stu-client-vue",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "test": "vue-tsc && vite build --mode=test",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "git add . && cz-customizable",
    "changelog": "conventional-changelog -p gitmoji-config -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@jvbjs/eslint-config": "^1.0.8",
    "@jvbjs/prettier-config": "^1.0.3",
    "@jvbjs/stylelint-config": "^1.0.3",
    "@types/node": "^20.4.6",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "commitlint-config-cz": "^0.13.3",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "conventional-changelog-cli": "^3.0.0",
    "conventional-changelog-gitmoji-config": "^1.5.2",
    "cz-customizable": "^7.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-define-options": "^1.3.14",
    "vite": "^4.4.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-vue-images": "^0.6.1",
    "vue-tsc": "^1.8.8"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "prettier": "@jvbjs/prettier-config"
}
